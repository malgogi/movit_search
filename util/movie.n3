{?A owl:differentFrom ?B} => {?B owl:differentFrom ?A}.
{?X owl:sameAs ?Y} => {?Y owl:sameAs ?X}.
{?X owl:sameAs ?Y. ?Y owl:sameAs ?Z} => {?X owl:sameAs ?Z}.
{?X owl:sameAs ?Y. ?X owl:differentFrom ?Y} => false.

{ ?A :name ?B; :date ?C;. ?D :name ?E; :date ?F. ( ?C ?F ) math:difference 0 } => { ?A owl:sameAs ?D }.
{ ?A :name ?B; :date ?C;. ?D :name ?E; :date ?F. ( ?C ?F ) math:difference [ math:notEqualTo 0 ] } => { ?A owl:differentFrom ?D }.
